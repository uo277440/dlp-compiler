@startuml Types
skinparam style strictuml
skinparam monochrome true

interface ASTNode {
    getLine(): int
    getColumn(): int
}
class ArrayType{
    size: int
}
class StructType {
    fields: List<StructField>
}
class StructField{
name: String
type: Type
}

interface Type

ASTNode <|-- Type

class IntType{
value: int
}
class CharType{
    value: char
        }
class DoubleType{
         value: double
      }
class FuncType{
params: VarDefinition;
returnType: Type;
}

Type <|.. IntType
Type <|.. CharType
Type <|.. DoubleType
Type <|.. FuncType
Type <|.. StructType
Type <|.. ArrayType

@enduml
